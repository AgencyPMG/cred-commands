{
    "name": "pmg/cred-commands",
    "description": "Symfony console commands to interact with the AWS SSM Paramter Store",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/console": "^4.4",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "PMG\\CredCommands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PMG\\CredCommands\\": "test/"
        }
    }
}
